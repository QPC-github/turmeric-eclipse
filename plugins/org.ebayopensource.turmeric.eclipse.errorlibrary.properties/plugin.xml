<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.ebayopensource.turmeric.eclipse.errorlibrary.soaErrorLibProvider">
      <SOAErrorLibraryProvider
            providerID="%ErrorLib.Provider.ID.Properties"
            providerImplementation="org.ebayopensource.turmeric.eclipse.errorlibrary.properties.providers.PropertiesContentSOAErrorLibraryProvider">
      </SOAErrorLibraryProvider>
   </extension>
   <extension
       point="org.ebayopensource.turmeric.eclipse.errorlibrary.soaErrorLibModelProvider">
    <SOAErrLibViewModelProvider
          errViewModelProvider="org.ebayopensource.turmeric.eclipse.errorlibrary.properties.view.PropertiesContentRegistry"
          providerID="%ErrorLib.Provider.ID.Properties">
    </SOAErrLibViewModelProvider>
 </extension>
 <extension
         id="TurmericErrorLibraryProjectBuilder"
         name="Turmeric Property Based Error Library Builder"
         point="org.eclipse.core.resources.builders">
      		<builder
              callOnEmptyDelta="false"
              hasNature="true">
         		<run class="org.ebayopensource.turmeric.eclipse.errorlibrary.properties.eclipse.TurmericErrorLibraryProjectBuilder"/>
      		</builder>
    </extension>
    
	<extension
         id="ErrorLibraryProjectNature"
         name="Error Library Project Nature"
         point="org.eclipse.core.resources.natures">
      		<runtime>
         		<run class="org.ebayopensource.turmeric.eclipse.errorlibrary.properties.eclipse.TurmericErrorLibraryProjectNature"/>
      		</runtime>
     		 <builder id="org.ebayopensource.turmeric.eclipse.errorlibrary.properties.TurmericErrorLibraryProjectBuilder"/>
   	</extension>
 	<extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="org.ebayopensource.turmeric.eclipse.errorlibrary.properties.ui.ErrorLibraryProjPropertyPage"
            id="org.ebayopensource.turmeric.eclipse.errorlibrary.properties.ui.errorlibraryProjectPropertyPage"
            name="%PropertyPage.ErrorLibrary.Name">
         <enabledWhen>
            <adapt
                  type="org.eclipse.core.resources.IProject">
               <test
                     property="org.eclipse.core.resources.projectNature"
                     value="%Turmeric.ProjectNature.ID.ErrorLibrary">
               </test>
            </adapt>
         </enabledWhen>
      </page>
   </extension>
  <extension
        point="org.ebayopensource.turmeric.eclipse.resources.soaProjectResolver">
     <soaProjectResolver
           projectNature="%Turmeric.ProjectNature.ID.ErrorLibrary"
           projectResolverClass="org.ebayopensource.turmeric.eclipse.errorlibrary.resources.model.SOAErrorLibraryProjectResolver">
     </soaProjectResolver>
  </extension>
  <extension
        point="org.eclipse.ui.preferencePages">
     <page
     	   category="org.ebayopensource.turmeric.eclipse.ui.preferences"
           class="org.ebayopensource.turmeric.eclipse.errorlibrary.properties.preferences.ErrorIdServicePreferencePage"
           id="org.ebayopensource.turmeric.eclipse.errorlibrary.properties.preferences.ErrorIdServicePreferencePage"
           name="ErrorIdService Preferences">
     </page>
  </extension>
  <extension
        point="org.eclipse.core.runtime.preferences">
     <initializer
           class="org.ebayopensource.turmeric.eclipse.errorlibrary.properties.preferences.ErrorIdServicePreferenceInitializer">
     </initializer>
  </extension>
</plugin>
